<h3>Pick from Below</h3>

<ul>
    <% resultsArr.forEach(feature => { %>
        <li>
            <div><%= feature.place_name %> </div>
            <div>category: <%= feature.properties.category %> </div>
            <div>foursquare: <a target="_blank" href="https://foursquare.com/v/<%= feature.properties.foursquare %>">link</a></div>

            <form action="/restaurants/<%=restaurantId%>/map/?_method=put" method="post">
                <input type="hidden" name="lon" value="<%=feature.center[0]%>">
                <input type="hidden" name="lat" value="<%=feature.center[1]%>">
                <button type="submit">Update Map With This Entry</button>    
            </form>
        </li>
    <% }) %>
</ul>
        

<section class="padding-top contact-section bg-dark">
    <div class="container px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5">

            <div class="col-md-4 mb-3 mb-md-0">
                <div class="card py-4 h-100">
                    <div class="card-body text-center">

                        <h4 class="text-uppercase m-0">  Name   </h4>

                        <hr class="my-4 mx-auto" />

                        form button

                    </div>
                </div>
            </div>

        </div>
    </div>
</section>